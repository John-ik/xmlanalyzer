name "xmlanalyzer"
description "D app for control a lot of xml configs"
authors "johnik"
copyright "Copyright Â© 2024, johnik"
license "proprietary"

subPackage "./lib-xmlanalyzer"
subPackage "./gui" 
subPackage "./cli"

targetPath "."

configuration "gui" {
    targetType "executable"
    versions "GuiApp"
    dependency ":gui" version="*"
}

configuration "cli" {
    targetType "executable"
    versions "CliApp"
    dependency ":cli" version="*"
}

configuration "lib" {
    targetType "library"
    versions "LibApp"
    dependency ":lib-xmlanalyzer" version="*"
}
